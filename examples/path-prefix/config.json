{
  "defaults": {
    "protocol": "http",
    "host": "localhost:3000",
    "path": "/path/prefix"
  },
  "facebook": {
    "key": "[APP_ID]",
    "secret": "[APP_SECRET]",
    "callback": "/facebook_callback",
    "scope": [
      "public_profile",
      "email"
    ]
  },
  "twitter": {
    "key": "[CONSUMER_KEY]",
    "secret": "[CONSUMER_SECRET]",
    "callback": "/twitter_callback"
  }
}
