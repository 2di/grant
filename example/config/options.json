{
  "facebook": {
    "scope": "publish_actions,publish_stream,read_stream,manage_pages,user_groups,friends_groups"
  },
  "linkedin": {
    "scope": "r_basicprofile,r_fullprofile,r_emailaddress,r_network,r_contactinfo,rw_nus,rw_company_admin,rw_groups,w_messages"
  },
  "twitter": {
  },
  "stocktwits": {
    "scope": "read,watch_lists,publish_messages,publish_watch_lists,follow_users,follow_stocks"
  },
  "soundcloud": {
    "redirect": "/connect/soundcloud/callback",
    "scope": "non-expiring"
  },
  "bitly": {
    "redirect": "/connect/bitly/callback"
  },
  "github": {
    "headers": {"User-Agent": "AppName"},
    "redirect": "/connect/github/callback"
  },
  "stackexchange": {
  },
  "google": {
    "redirect": "/connect/google/callback",
    "scope": [
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/youtube.readonly",
      "https://www.googleapis.com/auth/drive.readonly",
      "https://www.googleapis.com/auth/freebase",
      "https://www.googleapis.com/auth/tasks",
      "https://www.googleapis.com/auth/yt-analytics.readonly",
      "https://www.googleapis.com/auth/contacts.readonly"
    ]
  },
  "yahoo": {
  },
  "foursquare": {
  },
  "slack": {
  },
  "instagram": {
    "redirect": "/connect/instagram/callback"
  },
  "flickr": {
  },
  "disqus": {
    "redirect": "/connect/disqus/callback",
    "scope": "read,write"
  },
  "trello": {
  },
  "asana": {
    "redirect": "/connect/asana/callback"
  },
  "mailchimp": {
    "redirect": "/connect/mailchimp/callback"
  },
  "heroku": {
  },
  "dropbox": {
    "redirect": "/connect/dropbox/callback"
  }
}